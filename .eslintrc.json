{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
     "no-console": "off",
     "comma-dangle": "off",
     "quotes": "off",
     "react/prop-types": 0,
     "arrow-body-style": 0,
     "space-before-function-paren": 0
     },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ]
}
